{
  "name": "literat-dev-blog",
  "description": "Literat's site",
  "version": "1.0.0",
  "license": "0BSD",
  "homepage": "https://literat.dev",
  "private": true,
  "engines": {
    "node": "23.11.1"
  },
  "keywords": [
    "gatsby",
    "literat"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby serve",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "netlify": "netlify",
    "test": "npm-run-all --serial lint format test:unit lint:commit",
    "test:unit": "yarn test:unit:local",
    "test:unit:jest": "jest",
    "test:unit:ci": "yarn test:unit:coverage --runInBand",
    "test:unit:local": "yarn test:unit:coverage --maxWorkers=3",
    "test:unit:watch": "yarn test:unit:jest --watch",
    "test:unit:coverage": "yarn test:unit:jest --coverage",
    "lint": "npm-run-all --serial lint:scripts lint:markdown lint:text",
    "lint:scripts": "eslint ./",
    "lint:scripts:fix": "yarn lint:scripts --fix",
    "lint:commit": "yarn commitlint --from $(git describe --always --first-parent) --verbose",
    "lint:commit:last": "yarn commitlint --from HEAD~1 --to HEAD --verbose",
    "lint:markdown": "remark ./ --quiet",
    "lint:text": "textlint ./",
    "lint:text:fix": "yarn lint:text --fix",
    "format": "yarn format:check",
    "format:check": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "types": "tsc -p ./tsconfig.json",
    "post": "node ./scripts/createPost.mjs",
    "postinstall": "husky"
  },
  "dependencies": {
    "@mdx-js/react": "3.1.1",
    "@netlify/functions": "3.1.10",
    "@sparticuz/chromium": "138.0.2",
    "babel-plugin-styled-components": "2.1.4",
    "clsx": "2.1.1",
    "gatsby": "5.15.0",
    "gatsby-plugin-alias-imports": "1.0.5",
    "gatsby-plugin-feed": "5.15.0",
    "gatsby-plugin-image": "3.15.0",
    "gatsby-plugin-mdx": "5.15.0",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-react-helmet": "6.15.0",
    "gatsby-plugin-sass": "6.15.0",
    "gatsby-plugin-sharp": "5.15.0",
    "gatsby-plugin-styled-components": "6.15.0",
    "gatsby-remark-copy-linked-files": "6.15.0",
    "gatsby-remark-images": "7.15.0",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-vscode": "3.3.1",
    "gatsby-source-filesystem": "5.15.0",
    "gatsby-transformer-remark": "6.15.0",
    "gatsby-transformer-sharp": "5.15.0",
    "normalize.css": "8.0.1",
    "puppeteer-core": "^10.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "5.5.0",
    "react-is": "18.3.1",
    "resize-observer-polyfill": "1.5.1",
    "sass": "1.93.2",
    "slugify": "1.6.6",
    "styled-components": "6.1.19",
    "typescript": "5.9.2",
    "vscode-styled-components": "./vendor_modules/vscode-styled-components",
    "waait": "1.0.5"
  },
  "devDependencies": {
    "@almacareer/remark-config": "0.1.0",
    "@babel/core": "7.28.4",
    "@babel/eslint-parser": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@lmc-eu/prettier-config": "2.0.1",
    "@lmc-eu/textlint-rule-preset-lmc": "2.0.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/inquirer": "9.0.9",
    "@types/jest": "29.5.14",
    "@types/node": "22.18.6",
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.15.0",
    "chalk": "5.6.2",
    "core-js": "3.45.1",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-wesbos": "4.3.2",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "fs-extra": "11.3.2",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "12.9.6",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "netlify-cli": "17.38.1",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-toml": "2.0.6",
    "puppeteer": "^10.1.0",
    "remark-cli": "12.0.1",
    "remark-frontmatter": "5.0.0",
    "remark-mdx": "3.1.1",
    "textlint": "14.8.4",
    "ts-node": "10.9.2"
  },
  "packageManager": "yarn@4.10.3"
}
