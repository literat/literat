    "eslint-plugin-react-hooks": "catalog:lint",
    "fs-extra": "catalog:backend",
    "husky": "catalog:lint",
    "identity-obj-proxy": "catalog:test",
    "inquirer": "catalog:backend",
    "is-ci": "catalog:script",
    "jest": "catalog:test",
    "jest-environment-jsdom": "catalog:test",
    "netlify-cli": "catalog:script",
    "npm-run-all2": "catalog:script",
    "prettier": "catalog:lint",
    "prettier-plugin-toml": "catalog:lint",
    "puppeteer": "catalog:backend",
    "remark-cli": "catalog:lint",
    "remark-frontmatter": "catalog:lint",
