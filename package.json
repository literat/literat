{
  "name": "literat-dev-blog",
  "description": "Literat's site",
  "version": "1.0.0",
  "license": "0BSD",
  "homepage": "https://literat.dev",
  "private": true,
  "engines": {
    "node": "23.11.1"
  },
  "keywords": [
    "gatsby",
    "literat"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby serve",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "netlify": "netlify",
    "test": "npm-run-all --serial lint format test:unit lint:commit",
    "test:unit": "yarn test:unit:local",
    "test:unit:jest": "jest",
    "test:unit:ci": "yarn test:unit:coverage --runInBand",
    "test:unit:local": "yarn test:unit:coverage --maxWorkers=3",
    "test:unit:watch": "yarn test:unit:jest --watch",
    "test:unit:coverage": "yarn test:unit:jest --coverage",
    "lint": "npm-run-all --serial lint:scripts lint:markdown lint:text",
    "lint:scripts": "eslint ./",
    "lint:scripts:fix": "yarn lint:scripts --fix",
    "lint:commit": "yarn commitlint --color --verbose --from $(git merge-base origin/main HEAD)",
    "lint:commit:last": "yarn commitlint --from HEAD~1 --to HEAD --verbose",
    "lint:markdown": "remark ./ --quiet",
    "lint:text": "textlint ./",
    "lint:text:fix": "yarn lint:text --fix",
    "lint:lockfile": "yarn dlx lockfile-lint",
    "format": "yarn format:check",
    "format:check": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "types": "tsc -p ./tsconfig.json",
    "post": "node ./scripts/createPost.mjs",
    "postinstall:lockfile": "yarn lint:lockfile",
    "postinstall:hooks": "husky"
  },
  "dependencies": {
    "@mdx-js/react": "catalog:frontend",
    "@netlify/functions": "catalog:backend",
    "@sparticuz/chromium": "catalog:backend",
    "clsx": "catalog:frontend",
    "gatsby": "catalog:frontend",
    "gatsby-plugin-alias-imports": "catalog:frontend",
    "gatsby-plugin-feed": "catalog:frontend",
    "gatsby-plugin-image": "catalog:frontend",
    "gatsby-plugin-mdx": "catalog:frontend",
    "gatsby-plugin-netlify": "catalog:frontend",
    "gatsby-plugin-react-helmet": "catalog:frontend",
    "gatsby-plugin-sass": "catalog:frontend",
    "gatsby-plugin-sharp": "catalog:frontend",
    "gatsby-plugin-styled-components": "catalog:frontend",
    "gatsby-remark-copy-linked-files": "catalog:frontend",
    "gatsby-remark-images": "catalog:frontend",
    "gatsby-remark-reading-time": "catalog:frontend",
    "gatsby-remark-vscode": "catalog:frontend",
    "gatsby-source-filesystem": "catalog:frontend",
    "gatsby-transformer-remark": "catalog:frontend",
    "gatsby-transformer-sharp": "catalog:frontend",
    "normalize.css": "catalog:frontend",
    "puppeteer-core": "catalog:backend",
    "react": "catalog:frontend",
    "react-dom": "catalog:frontend",
    "react-helmet": "catalog:frontend",
    "react-icons": "catalog:frontend",
    "resize-observer-polyfill": "catalog:frontend",
    "sass": "catalog:frontend",
    "slugify": "catalog:backend",
    "styled-components": "catalog:frontend",
    "vscode-styled-components": "catalog:frontend",
    "waait": "catalog:backend"
  },
  "devDependencies": {
    "@almacareer/remark-config": "catalog:lint",
    "@babel/core": "catalog:test",
    "@babel/eslint-parser": "catalog:test",
    "@babel/preset-env": "catalog:test",
    "@babel/preset-react": "catalog:test",
    "@babel/preset-typescript": "catalog:test",
    "@commitlint/cli": "catalog:lint",
    "@commitlint/config-conventional": "catalog:lint",
    "@eslint/eslintrc": "catalog:lint",
    "@lmc-eu/prettier-config": "catalog:lint",
    "@lmc-eu/textlint-rule-preset-lmc": "catalog:lint",
    "@testing-library/dom": "catalog:test",
    "@testing-library/jest-dom": "catalog:test",
    "@testing-library/react": "catalog:test",
    "@types/inquirer": "catalog:types",
    "@types/jest": "catalog:types",
    "@types/node": "catalog:types",
    "@types/react": "catalog:types",
    "@types/react-dom": "catalog:types",
    "@types/styled-components": "catalog:types",
    "@typescript-eslint/eslint-plugin": "catalog:lint",
    "@typescript-eslint/parser": "catalog:lint",
    "babel-jest": "catalog:test",
    "babel-plugin-styled-components": "catalog:test",
    "babel-preset-gatsby": "catalog:test",
    "chalk": "catalog:backend",
    "core-js": "catalog:build",
    "eslint": "catalog:lint",
    "eslint-config-airbnb": "catalog:lint",
    "eslint-config-airbnb-typescript": "catalog:lint",
    "eslint-config-prettier": "catalog:lint",
