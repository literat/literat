{
  "name": "literat-dev-blog",
  "description": "Literat's site",
  "version": "1.0.0",
  "license": "0BSD",
  "homepage": "https://literat.dev",
  "keywords": [
    "gatsby",
    "literat"
  ],
  "workspaces": [
    "functions/social-image"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "netlify": "netlify",
    "test": "npm-run-all --serial lint format test:unit lint:commit",
    "test:unit": "yarn test:unit:local",
    "test:unit:jest": "jest --config config/jest/config.js",
    "test:unit:ci": "yarn test:unit:coverage --runInBand",
    "test:unit:local": "yarn test:unit:coverage --maxWorkers=3",
    "test:unit:watch": "yarn test:unit:jest --watch",
    "test:unit:coverage": "yarn test:unit:jest --coverage",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "lint:commit": "yarn commitlint --from $(git describe --always --first-parent) --verbose",
    "lint:commit:last": "yarn commitlint --from HEAD~1 --to HEAD --verbose",
    "format": "yarn format:check",
    "format:check": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "types": "tsc -p ./tsconfig.json",
    "post": "node ./scripts/createPost.mjs"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@netlify/functions": "^1.0.0",
    "chrome-aws-lambda": "^10.1.0",
    "gatsby": "4.14.1",
    "gatsby-plugin-image": "2.14.1",
    "gatsby-plugin-mdx": "3.14.0",
    "gatsby-plugin-netlify": "^4.4.0",
    "gatsby-plugin-sharp": "4.14.1",
    "gatsby-remark-copy-linked-files": "5.14.0",
    "gatsby-remark-images": "6.14.0",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-vscode": "3.3.1",
    "gatsby-source-filesystem": "4.14.0",
    "gatsby-transformer-remark": "5.14.0",
    "gatsby-transformer-sharp": "4.14.0",
    "normalize.css": "8.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
    "react-is": "18.1.0",
    "slugify": "1.6.5",
    "styled-components": "5.3.5",
    "typescript": "4.6.4",
    "vsc-community-material-theme": "git+https://github.com/material-theme/vsc-community-material-theme.git",
    "vscode-styled-components": "git+https://github.com/styled-components/vscode-styled-components.git",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.0",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.18.0",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@lmc-eu/prettier-config": "1.2.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "27.5.1",
    "@types/node": "16.11.36",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-preset-gatsby": "2.14.0",
    "chalk": "^5.0.1",
    "core-js": "3.22.6",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-wesbos": "3.0.2",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "8.2.4",
    "jest": "27.5.1",
    "netlify-cli": "^10.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2"
  },
  "packageManager": "yarn@3.2.1"
}
