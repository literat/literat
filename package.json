{
  "name": "literat-dev-blog",
  "description": "Literat's site",
  "version": "1.0.0",
  "license": "0BSD",
  "homepage": "https://literat.dev",
  "private": true,
  "keywords": [
    "gatsby",
    "literat"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "netlify": "netlify",
    "test": "npm-run-all --serial lint format test:unit lint:commit",
    "test:unit": "yarn test:unit:local",
    "test:unit:jest": "jest --config config/jest/config.js",
    "test:unit:ci": "yarn test:unit:coverage --runInBand",
    "test:unit:local": "yarn test:unit:coverage --maxWorkers=3",
    "test:unit:watch": "yarn test:unit:jest --watch",
    "test:unit:coverage": "yarn test:unit:jest --coverage",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "lint:commit": "yarn commitlint --from $(git describe --always --first-parent) --verbose",
    "lint:commit:last": "yarn commitlint --from HEAD~1 --to HEAD --verbose",
    "format": "yarn format:check",
    "format:check": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "types": "tsc -p ./tsconfig.json",
    "post": "node ./scripts/createPost.mjs"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@netlify/functions": "^1.0.0",
    "chrome-aws-lambda": "^10.1.0",
    "gatsby": "4.24.8",
    "gatsby-plugin-image": "2.24.0",
    "gatsby-plugin-mdx": "3.20.0",
    "gatsby-plugin-netlify": "5.0.1",
    "gatsby-plugin-react-helmet": "^5.15.0",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-remark-copy-linked-files": "5.24.0",
    "gatsby-remark-images": "6.24.0",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-vscode": "3.3.1",
    "gatsby-source-filesystem": "4.24.0",
    "gatsby-transformer-remark": "5.25.1",
    "gatsby-transformer-sharp": "4.24.0",
    "normalize.css": "8.0.1",
    "puppeteer-core": "^10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.6.0",
    "react-is": "18.2.0",
    "slugify": "1.6.5",
    "styled-components": "5.3.6",
    "typescript": "4.9.3",
    "vsc-community-material-theme": "git+https://github.com/material-theme/vsc-community-material-theme.git",
    "vscode-styled-components": "git+https://github.com/styled-components/vscode-styled-components.git",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@lmc-eu/prettier-config": "1.2.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/inquirer": "9.0.3",
    "@types/jest": "29.2.3",
    "@types/node": "16.18.3",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.3.1",
    "babel-preset-gatsby": "2.24.0",
    "chalk": "^5.0.1",
    "core-js": "3.26.1",
    "eslint": "8.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-wesbos": "3.1.4",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "10.1.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "9.1.4",
    "is-ci": "^3.0.1",
    "jest": "29.3.1",
    "jest-environment-jsdom": "^29.0.0",
    "netlify-cli": "12.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "puppeteer": "^10.1.0"
  },
  "packageManager": "pnpm@7.29.3"
}
