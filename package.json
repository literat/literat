{
  "name": "literat-dev-blog",
  "description": "Literat's site",
  "version": "1.0.0",
  "license": "0BSD",
  "homepage": "https://literat.dev",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "gatsby",
    "literat"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby serve",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "netlify": "netlify",
    "test": "npm-run-all --serial lint format test:unit lint:commit",
    "test:unit": "yarn test:unit:local",
    "test:unit:jest": "jest --config config/jest/config.js",
    "test:unit:ci": "yarn test:unit:coverage --runInBand",
    "test:unit:local": "yarn test:unit:coverage --maxWorkers=3",
    "test:unit:watch": "yarn test:unit:jest --watch",
    "test:unit:coverage": "yarn test:unit:jest --coverage",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix",
    "lint:commit": "yarn commitlint --from $(git describe --always --first-parent) --verbose",
    "lint:commit:last": "yarn commitlint --from HEAD~1 --to HEAD --verbose",
    "format": "yarn format:check",
    "format:check": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "types": "tsc -p ./tsconfig.json",
    "post": "node ./scripts/createPost.mjs",
    "postinstall": "husky"
  },
  "dependencies": {
    "@mdx-js/react": "2.3.0",
    "@netlify/functions": "^2.0.0",
    "@sparticuz/chromium": "^123.0.1",
    "babel-plugin-styled-components": "^2.1.4",
    "gatsby": "5.13.4",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-mdx": "5.13.1",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-react-helmet": "^6.0.0",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-plugin-styled-components": "^6.13.1",
    "gatsby-remark-copy-linked-files": "6.13.1",
    "gatsby-remark-images": "7.13.1",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-vscode": "3.3.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-remark": "6.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "normalize.css": "8.0.1",
    "puppeteer-core": "^10.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "5.2.0",
    "react-is": "18.3.1",
    "slugify": "1.6.6",
    "styled-components": "6.1.9",
    "typescript": "5.4.5",
    "vscode-styled-components": "./vendor_modules/vscode-styled-components",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/eslint-parser": "7.24.5",
    "@babel/preset-env": "7.24.5",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@lmc-eu/prettier-config": "2.0.0",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.6",
    "@types/inquirer": "9.0.7",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.8",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.13.2",
    "chalk": "^5.0.1",
    "core-js": "3.37.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-wesbos": "4.0.1",
    "eslint-plugin-html": "8.1.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "fs-extra": "11.2.0",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
    "inquirer": "9.2.20",
    "is-ci": "^3.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.0.0",
    "netlify-cli": "17.23.1",
    "npm-run-all2": "6.1.2",
    "prettier": "3.2.5",
    "puppeteer": "^10.1.0"
  },
  "packageManager": "yarn@4.2.1"
}
